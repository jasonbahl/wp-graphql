{
    "$schema": "https://unpkg.com/@changesets/config@2.3.1/schema.json",
    "changelog": "./scripts/changelog-formatter.js",
    "commit": false,
    "fixed": [],
    "linked": [],
    "access": "public",
    "baseBranch": "develop",
